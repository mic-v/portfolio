---
import "../styles/main.css";
import constructionImg from "../assets/Treadmillcrane.jpg";
---
<style>
  #under-construction {
    display: flex;
    flex-direction: column;
    align-items: center; 
    gap: 2em;
    width: 100%;
  }

  #under-construction h3 {
    color: #BB4430; 
  }

  #under-construction img {
    width: 300px;
  }
</style>

<div id="under-construction">
  <h3>Currenly Under Construction</h3>
  <img src={constructionImg.src}/>

</div>
